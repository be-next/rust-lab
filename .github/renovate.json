{
    "extends": [
        "config:best-practices"
    ],
    "enabledManagers": [
        "cargo"
    ],
    "packageRules": [
        {
            "managers": [
                "cargo"
            ],
            "packagePatterns": [
                "*"
            ],
            "groupName": "all dependencies",
            "schedule": [
                "at any time"
            ]
        }
    ],
    "rust": {
        "enabled": true
    },
    "autodiscover": true,
    "autodiscoverFilter": [
        "rust-lab/**"
    ],
    "ignorePaths": [],
    "rangeStrategy": "bump"
}